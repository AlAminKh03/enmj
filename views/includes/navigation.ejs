<header class="main-header">
        <nav class="main-header__nav">
            <ul class="main-header__item-list">
                <li class="main-header__item"><a class="<%= path==='/'? 'active': ' '%>" href="/">Shop</a></li>
                <li class="main-header__item"><a class="<%= path==='/product-list'? 'active': ' '%>" href="/product-list">Products</a></li>
                <% if (isAuthenticated) { %>
                <li class="main-header__item"><a class="<%= path==='/cart'? 'active': ' '%>" href="/cart">Cart</a></li>
                <li class="main-header__item"><a class="<%= path==='/order'? 'active': ' '%>" href="/order">Order</a></li>
                <li class="main-header__item"><a class="<%= path==='/admin/add-product'? 'active': ' '%>" href="/admin/add-product">Add products</a></li>
                <li class="main-header__item"><a class="<%= path==='/admin/products'? 'active': ' '%>" href="/admin/products">Admin Products</a></li>
                <% } %>
            </ul>
            <ul class="main-header__item-list">
                <% if (!isAuthenticated) { %>
                <li class="main-header__item">
                    <a class="<%= path === '/login' ? 'active' : '' %>" href="/login">Login</a>
                </li>
                <li class="main-header__item">
                    <a class="<%= path === '/signup' ? 'active' : '' %>" href="/signup">Signup</a>
                </li>
                <% } else { %>
              <form action="/logout" method="post">
                <li class="main-header__item">
                    <button >LogOut</button>
                </li>
              </form>
              <% } %>
            </ul>
        </nav>
</header>